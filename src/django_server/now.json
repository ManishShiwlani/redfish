{
  "version": 2,
  "name": "redfish-server-prod",
  "builds": [
    {
      "src": "django_server/wsgi.py",
      "use": "@ardnt/now-python-wsgi",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "django_server/wsgi.py"
    }
  ],
  "env": {
    "ENV": "production"
  }
}
