{
  "version": 2,
  "name": "redfish-django-server-productin",
  "builds": [
    {
      "src": "django_server/wsgi/production.py",
      "use": "@ardnt/now-python-wsgi",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "django_server/wsgi/production.py"
    }
  ]
}
